{
  "name": "second_project",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist",
    "build": "npx tsc",
    "dev-startup":"db-migrate up",
    "dev-resetdb":"db-migrate reset",
    "test": "npm run dbtest-up && npm run build && NODE_ENV=test jasmine && npm run dbtest-down",
    "dbtest-up":"db-migrate --config ./database.json --e test up",
    "dbtest-down":"db-migrate reset --config ./database.json --e test",
    "tsc": "tsc",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --config .prettierrc --check '**/*.ts'",
    "prettier:fix": "prettier --write '**/*.ts'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0"
  }
}
